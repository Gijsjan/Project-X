.input-append.control-group
	input.select(class!="span<%= span %>", type="text", value!="<%= value %>")
	button.btn.btn-narrow
		span.caret
ul.dropdown-menu(role="menu")
	|<% selectoptions.each(function(option) { %>
	li(data-id!="<%=option.id%>")
		a(tabindex="-1") <%= option.get('value') %>
	|<% }); %>


